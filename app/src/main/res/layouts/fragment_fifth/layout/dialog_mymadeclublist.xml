<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/drawable_dialog">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/clubTop10TitleTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/seven_l"
            android:text="# 내가 만든 클럽리스트"
            android:textColor="@color/black"
            android:textSize="15dp" />

        <com.loopeer.cardstack.CardStackView
            android:id="@+id/top10ClubListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/clubTop10TitleTextView"
            android:layout_marginTop="10dp"
            android:background="@android:color/transparent"
            android:clipToPadding="false"
            android:nestedScrollingEnabled="false"
            android:paddingLeft="12dp"
            android:paddingTop="12dp"
            android:paddingRight="12dp" />

    </RelativeLayout>
</layout>