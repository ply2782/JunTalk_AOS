    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="bulletinCommentModel"
            type="com.cross.juntalk2.model.BulletinCommentModel" />


    </data>

    <RelativeLayout
